{
  "name": "@molit/genomics-reporting-viewer",
  "version": "1.0.13",
  "description": "Components for displaying FHIR genomic reports complying to the FHIR Genomics Reporting Implementation Guide http://hl7.org/fhir/uv/genomics-reporting/.",
  "keywords": [
    "genomics",
    "genomics reporting",
    "genomics report",
    "fhir",
    "stencil",
    "component"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/genomics-reporting-viewer/genomics-reporting-viewer.js",
  "files": [
    "dist/",
    "loader/"
